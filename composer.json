{
  "name": "agile-cqrs-es",
  "description": "generate common files in cqrs + es",
  "license": "",
  "keywords": [
    "cqrs",
    "es",
    "generate",
    "agile"
  ],
  "homepage": "http://github.com/nclundsten/es",
  "autoload": {
    "psr-4": {
        "CqrsEsAgility\\": "src/CqrsEsAgility"
    }
  },
  "require": {
    "php": "^7.0.0",
    "prooph/event-sourcing": "^4.0",
    "prooph/event-store": "^6.5",
    "prooph/event-store-bus-bridge": "^2.0",
    "prooph/service-bus": "^5.2",
    "nikic/php-parser" : "dev-master",
    "zendframework/zend-code" : "dev-master",
    "container-interop/container-interop" : "^1.2.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": "^4.8.21"
  },
  "suggest": {
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload-dev": {
    "psr-4": {
    }
  },
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  }
}